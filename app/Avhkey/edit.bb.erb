<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @avhkey.object %>
    <h1>Edit <%= @avhkey.keyname%></h1>
    <%= link_to "Delete", :action => :delete, :id => @avhkey.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @avhkey.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Keyname: </td>
              <td class="itemValue"><input type="text" name="avhkey[keyname]" value="<%= @avhkey.keyname %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Flts: </td>
              <td class="itemValue"><input type="text" name="avhkey[flts]" value="<%= @avhkey.flts %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
